{
  "name": "simple-starter",
  "version": "0.3.0",
  "description": "Basic boilerplate to start development (pug + sass)",
  "homepage": "https://github.com/koshikishi/simple-starter",
  "bugs": {
    "url": "https://github.com/koshikishi/simple-starter/issues"
  },
  "license": "MIT",
  "author": "Anton Northern",
  "repository": "github:koshikishi/simple-starter",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "pug-lint": "pug-lint source/*.pug source/pug/**/*.pug",
    "eslint": "eslint source/js/",
    "stylelint": "stylelint \"source/sass/**/*.scss\"",
    "test": "npm run editorconfig && npm run pug-lint && npm run eslint && npm run stylelint",
    "build": "cross-env NODE_ENV=production gulp build",
    "start": "cross-env NODE_ENV=development gulp"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.7",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.16",
    "del": "^6.0.0",
    "eslint": "^8.8.0",
    "gulp": "^4.0.2",
    "gulp-html-minifier-terser": "^6.0.1",
    "gulp-libsquoosh": "^1.0.12",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-pug": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svgmin": "^4.1.0",
    "gulp-svgstore": "^9.0.0",
    "path": "^0.12.7",
    "postcss": "^8.4.6",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.3.1",
    "pug-lint": "^2.6.0",
    "sass": "^1.49.7",
    "stylelint": "^14.3.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "webpack": "^5.68.0",
    "webpack-stream": "^7.0.0"
  },
  "engines": {
    "node": "^12 || ^14"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.pug",
    "source/pug/**/*.pug",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.scss"
  ]
}
