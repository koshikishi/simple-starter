include mixins

block variables
  -
    const title = '';
    const description = '';

doctype html
html(lang="en")
  head
    block head
      meta(charset="utf-8")
      meta(name="viewport" content="width=device-width, initial-scale=1")
      meta(name="description" content=description)
      title= title
      link(rel="stylesheet" href="css/style.min.css")

  body
    block content
    block scripts
